    }
    
    if (filters?.toDate) {
      query += ` AND wu.usage_date <= $${paramIndex}`;
      params.push(filters.toDate.toISOString().split('T')[0]);
      paramIndex++;
    }
    
    query += ` ORDER BY wu.usage_date DESC, wu.created_at DESC`;
    
    const result = await pool.query(query, params);
    return result.rows || [];
  }

