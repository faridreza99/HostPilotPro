  private groupBy<T>(array: T[], key: keyof T): [string, T[]][] {
    const groups = array.reduce((result, item) => {
      const group = String(item[key]);
      if (!result[group]) {
        result[group] = [];
      }
      result[group].push(item);
      return result;
    }, {} as Record<string, T[]>);
    
    return Object.entries(groups);
  }

